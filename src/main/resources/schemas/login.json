{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Login",
    "description": "A login request",
    "type": "object",
    "properties": {
        "email": {
            "description": "The email of the user",
            "type": "string",
            "format": "email"
        },
        "password": {
            "description": "The password of the user",
            "type": "string"
        }
    },
    "required": ["email", "password"]
}
